# Stuff
alias la='ls -a'
alias ll='ls -al'

# Navigation
alias ..="cd .."
alias ...="cd ../.."
alias dots="cd ~/.dotfiles"

# Abstract
alias abs="cd ~/Code/Go/src/github.com/goabstract"
alias repos="cd ~/Library/Application Support/Abstract/repost"

# Bundler
alias b="bundle"

# Rails
alias migrate="rake db:migrate db:rollback && rake db:migrate"
alias rk="rake"

# Sublime
alias s="subl ."

# Airport
alias airport="/System/Library/PrivateFrameworks/Apple80211.framework/Versions/Current/Resources/airport"

alias ack="ag"

alias cp-branch="git current-branch | tr -d '\n' | pbcopy"
